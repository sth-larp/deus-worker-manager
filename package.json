    "deus-model-engine": "github:sth-larp/deus-model-engine",
    "express": "^4.16.3",
    "forever": "^0.15.3",
    "generic-pool": "^3.4.2",
    "glob": "^7.1.2",
    "meow": "^4.0.0",
    "nano": "^6.4.3",
    "npm": "^5.8.0",
    "reflect-metadata": "^0.1.12",
