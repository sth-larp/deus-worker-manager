    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.3",
    "pouchdb": "^6.4.3",
    "pouchdb-adapter-memory": "^6.4.3",
    "ts-node": "^5.0.1",
