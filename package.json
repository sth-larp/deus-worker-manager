{
  "name": "deus-worker-manager",
  "version": "0.0.1",
  "description": "DeusEx model workers manager",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "forever start --pidFile deus-worker-manager.pid build/worker_manager_runner.js -c '../config.js'",
    "stop": "forever stop build/worker_manager_runner.js",
    "dev": "nodemon --config nodemon.json",
    "db": "ts-node src/db/sync.ts -c ../../config.js"
  },
  "author": "",
  "license": "Apache2",
  "dependencies": {
    "deus-model-engine": "git+https://github.com/sth-larp/deus-model-engine.git",
    "forever": "^0.15.3",
    "generic-pool": "^3.1.7",
    "glob": "^7.1.2",
    "meow": "^3.7.0",
    "nano": "^6.2.0",
    "rxjs": "^5.4.0"
  },
  "devDependencies": {
    "@types/glob": "^5.0.30",
    "@types/meow": "^3.6.2",
    "@types/node": "^7.0.21",
    "nodemon": "^1.11.0",
    "ts-node": "^3.0.4",
    "typescript": "^2.3.3"
  }
}
